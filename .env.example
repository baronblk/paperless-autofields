# Paperless-NGX API Configuration
PAPERLESS_API_URL=http://localhost:8000
PAPERLESS_API_TOKEN=your_api_token_here

# Document Processing
DOCUMENT_TYPE=Rechnung
RUN_INTERVAL=300

# Web UI
WEB_PORT=5000
WEB_HOST=0.0.0.0

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/paperless-autofields.log
LOG_MAX_SIZE=10MB
LOG_BACKUP_COUNT=5

# Pattern File
PATTERN_FILE=patterns.yaml

# Processing Options
VALIDATE_EXTRACTED_VALUES=true
SKIP_PROCESSED_DOCUMENTS=true
